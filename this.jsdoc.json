{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["../modV-master/src", "../modV-master/README.md"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "./node_modules/jsdoc/plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "template": "./",
        "recurse": true
    }
}